{
  "name": "@henrotaym/laravel-package-scaffolder",
  "version": "0.0.0",
  "description": "laravel-package-scaffolder",
  "files": [
    "dist",
    "stubs"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "types": "dist/index.d.ts",
  "bin": {
    "laravel-package-scaffolder": "./bin/laravel-package-scaffolder.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "vite",
    "lint": "tsc && eslint . --fix",
    "format": "prettier --write \"src/**/*\"",
    "validate": "bun run lint && bun run format",
    "build": "tsup",
    "release": "bun run build && changeset publish",
    "test": "vitest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/henrotaym/laravel-package-scaffolder.git"
  },
  "author": "Henrotay Mathieu <mathieu.henrotay@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/henrotaym/laravel-package-scaffolder/issues"
  },
  "homepage": "https://github.com/henrotaym/laravel-package-scaffolder#readme",
  "devDependencies": {
    "@changesets/cli": "^2.26.1",
    "@types/node": "^18.16.3",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "postcss": "^8.4.23",
    "prettier": "^2.5.1",
    "tailwindcss": "^3.3.2",
    "tsup": "^6.7.0",
    "typescript": "^5.0.4",
    "vite": "^4.3.2",
    "vite-plugin-eslint": "^1.8.1",
    "vitest": "^0.31.2"
  },
  "dependencies": {
    "@henrotaym/scaffolding-utils": "^1.2.1"
  },
  "trustedDependencies": [
    "core-js",
    "esbuild"
  ]
}
